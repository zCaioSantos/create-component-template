{
 "publisher": "zCaioSantos",
 "name": "create-component-template",
 "displayName": "Create Component Template",
 "description": "Simplify React component creation with this template generator for VSCode. Simply set the component's name, and the extension will take care of the rest.",
 "version": "2.0.0",
 "engines": {
  "vscode": "^1.79.0"
 },
 "categories": [
  "Other"
 ],
 "license": "MIT",
 "files": [
  "LICENSE"
 ],
 "icon": "icon.png",
 "activationEvents": [],
 "main": "./dist/extension.js",
 "repository": {
  "type": "git",
  "url": "https://github.com/zCaioSantos/react-component-template.git"
 },
 "contributes": {
  "commands": [
   {
    "command": "react-component-template.reactComponentTemplate",
    "title": "Create Component Template"
   },
   {
    "command": "react-component-template.openTemplate",
    "title": "Open File Component Template"
   }
  ],
  "menus": {
   "explorer/context": [
    {
     "command": "react-component-template.reactComponentTemplate",
     "when": "explorerResourceIsFolder"
    },
    {
     "command": "react-component-template.openTemplate",
     "when": "explorerResourceIsFolder"
    }
   ]
  }
 },
 "scripts": {
  "vscode:prepublish": "npm run package",
  "compile": "webpack",
  "watch": "webpack --watch",
  "package": "webpack --mode production --devtool hidden-source-map",
  "compile-tests": "tsc -p . --outDir out",
  "watch-tests": "tsc -p . -w --outDir out",
  "pretest": "npm run compile-tests && npm run compile && npm run lint",
  "lint": "eslint src --ext ts",
  "test": "node ./out/test/runTest.js"
 },
 "devDependencies": {
  "@types/vscode": "^1.79.0",
  "@types/glob": "^8.1.0",
  "@types/mocha": "^10.0.1",
  "@types/node": "20.2.5",
  "@typescript-eslint/eslint-plugin": "^5.59.8",
  "@typescript-eslint/parser": "^5.59.8",
  "eslint": "^8.41.0",
  "glob": "^8.1.0",
  "mocha": "^10.2.0",
  "typescript": "^5.1.3",
  "ts-loader": "^9.4.3",
  "webpack": "^5.85.0",
  "webpack-cli": "^5.1.1",
  "@vscode/test-electron": "^2.3.2"
 }
}
